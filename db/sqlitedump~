CREATE TABLE "posts" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "text_content" text, "image_path" varchar(100), "video_path" varchar(100), "spark_count" integer, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL, "score" decimal, "views" integer);
CREATE TABLE "vibes" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "vibe" varchar(20), "user_id" integer, "post_id" integer, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE TABLE "comments" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "text_content" text, "author_name" varchar(50), "date_submitted" datetime, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL, "post_id" integer);
CREATE TABLE "widgets" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "name" varchar, "description" text, "stock" integer, "created_at" datetime, "updated_at" datetime);
CREATE UNIQUE INDEX "unique_schema_migrations" ON "schema_migrations" ("version");
CREATE INDEX "index_vibes_on_user_id" ON "vibes" ("user_id");
CREATE INDEX "index_vibes_on_post_id" ON "vibes" ("post_id");
CREATE UNIQUE INDEX "index_users_on_email" ON "users" ("email");
CREATE UNIQUE INDEX "index_users_on_reset_password_token" ON "users" ("reset_password_token");
