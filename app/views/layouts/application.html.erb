<!DOCTYPE html>
<html>
	<head>
		<title>
			<%= yield(:title) %>
		</title>
		<%= stylesheet_link_tag "bootstrap.min.css", "bootstrap-theme.min.css", "application", "default.css"  %>
        <%= javascript_include_tag "jquery-2.1.1.js", "bootstrap.min.js", "application" %>
		<%= csrf_meta_tags %>
		<%= yield :head %>
	</head>
	<body>
		<div id="wrap">
			<%= render 'layouts/header' %>

			<%= yield %>
			<div id="push"></div>
		</div>
		<%= render 'layouts/footer' %>
	</body>
</html>